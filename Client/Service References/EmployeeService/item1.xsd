        <xs:element xmlns:q3="http://MyCompany.com/Employee" minOccurs="0" ref="q3:Gender" />
        <xs:element xmlns:q4="http://MyCompany.com/Employee" minOccurs="0" ref="q4:DOB" />
        <xs:element xmlns:q5="http://MyCompany.com/Employee" minOccurs="0" ref="q5:Type" />
        <xs:element xmlns:q6="http://MyCompany.com/Employee" minOccurs="0" ref="q6:AnnualSalary" />
        <xs:element xmlns:q7="http://MyCompany.com/Employee" minOccurs="0" ref="q7:HourlyPay" />
        <xs:element xmlns:q8="http://MyCompany.com/Employee" minOccurs="0" ref="q8:HoursWorked" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>